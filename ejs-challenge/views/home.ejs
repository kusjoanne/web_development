<%- include("partials/header.ejs") -%>
<h1>Home</h1>

<p><%= homeStartingContent %></p>

<% posts.forEach(function(post){ %>
  <h1><%= post.entryTitle %></h1>
  <% post.entryText = post.entryText.substring(0,100)+"..."; %>
  <p><%= post.entryText %>
    <a href="/posts/<%=post.entryTitle%>">Read More</a>
  </p>
<% }); %>
<%- include("partials/footer.ejs") -%>
